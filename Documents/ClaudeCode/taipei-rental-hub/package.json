{
  "name": "taipei-rental-hub",
  "version": "1.0.0",
  "description": "台北租屋整合平台 - AI 驅動的智能租屋資訊聚合系統",
  "main": "src/backend/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:crawler\"",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:crawler": "nodemon src/crawler/index.js",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node src/backend/server.js",
    "crawler": "node src/crawler/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "node src/database/seed.js",
    "build": "npm run db:generate && cd frontend && npm run build",
    "start": "node start-production.js",
    "start:production": "node start-production.js",
    "db:migrate:deploy": "prisma migrate deploy"
  },
  "keywords": [
    "rental",
    "taipei",
    "real-estate",
    "crawler",
    "ai",
    "stagehand",
    "mursfoto"
  ],
  "author": "murs",
  "license": "MIT",
  "dependencies": {
    "@browserbasehq/stagehand": "latest",
    "@prisma/client": "^5.7.0",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "puppeteer": "^21.6.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}